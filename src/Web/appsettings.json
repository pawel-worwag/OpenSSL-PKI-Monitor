{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "CA": {
    "CertificationAuthorities": [
      {
        "Name": "CA1",
        "Certificates": [
          "/xxx/ca1/cert"
        ],
        "Crls": [
          "/xxx/ca1/crl",
          "/xxx/ca1/crl2"
        ],
        "IndexPath": "/xxx/ca1/index.txt"
      },
      {
        "Name": "CA2",
        "Certificates": [
          "/xxx/ca2/cert"
        ],
        "Crls": [
          "/xxx/ca2/crl"
        ],
        "IndexPath": "/xxx/ca2/index.txt"
      }
    ]
  }
}
